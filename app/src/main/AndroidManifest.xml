<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.choiceservices_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CadastroActivity"></activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/menu_main" />

        <activity
            android:name=".HomeAdmActivity"
            android:label="@string/title_activity_home_adm"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ListaActivity"
            android:label="@string/title_activity_lista"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CarrinhoActivity" />
        <activity android:name=".FormularioActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>